{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "hmm0-types": ["./hmm0-srv/src/models"],
      "hmm0-types/*": [
        "./hmm0-srv/src/models/*"
      ],
      "*": [
        "./src/*"
      ],
    },
  },
  "exclude": ["dist", "hmm0-srv"],
  "include": ["hmm0-srv/src/models/*", "./src/*"]
}
